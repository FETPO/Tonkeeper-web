{
  "name": "__MSG_appName__",
  "short_name": "__MSG_appName__",
  "description": "__MSG_appExtensionDescription__",
  "default_locale": "en",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "__MSG_actionTitle__"
